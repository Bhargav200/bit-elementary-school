#include<stdio.h>
int main()
{
	int m;
	printf("Enter the marks\n");//taking marks as input from user
	scanf("%d",&m);
	if(m>=85 && m<=100)//checking 1st condition
		printf("Grade A\n");
	else if(m>=70 && m<85)//checking 2nd condition
		printf("Grade B\n");
	else if(m>=55 && m<70)//checking 3rd condition
		printf("Grade C\n");
	else if(m>=40 && m<55)//checking 4th condition
		printf("Grade D\n");
	else //default
		printf("Grade F");
	return 0;
	
}
